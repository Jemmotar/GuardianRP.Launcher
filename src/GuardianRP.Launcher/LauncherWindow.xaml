        xmlns:Control="clr-namespace:GuardianRP.Launcher.UI.Control;assembly=GuardianRP.Launcher.UI" x:Class="GuardianRP.Launcher.MainWindow"
        mc:Ignorable="d"
        Title="GuardianRP.Launcher" Height="350" Width="525" ResizeMode="CanMinimize" Loaded="OnLoaded">
