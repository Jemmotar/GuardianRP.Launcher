        <Control:LoadingBar x:Name="lb" HorizontalAlignment="Left" Margin="55,235,0,0" VerticalAlignment="Top" Height="50" Width="380"/>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="23" Margin="105,155,0,0" TextWrapping="Wrap" Text="80" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="button" Content="Button" HorizontalAlignment="Left" Margin="240,158,0,0" VerticalAlignment="Top" Width="75" Click="button_Click"/>
    </Grid>
</Window>
