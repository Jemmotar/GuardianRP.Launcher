        <Image x:Name="ImageButton" HorizontalAlignment="Left" Height="30" Margin="0" VerticalAlignment="Top" Width="30" Source="/GuardianRP.UI;component/Resources/PanelButtonExit.PNG" Stretch="None"/>
    </Viewbox>
