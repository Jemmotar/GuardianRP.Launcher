             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:GuardianRP.UI.Control"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300" Width="30" Height="30" MouseDown="OnMouseDown" MouseUp="OnMouseUp" Loaded="OnLoaded" Cursor="Hand">
